{
  "name": "global-trade-network",
  "x-type": "hlfv1",
  "description": "The network to be in if you want to stay in the global trade business",
  "version": "1.0.0",
  "client": {
    "organization": "Org1",
    "credentialStore": {
      "path": "/tmp/hfc-kvs",
      "cryptoStore": {
        "path": "/tmp/hfc-cvs"
      },
      "wallet": "wallet-name"
    }
  },
  "channels": {
    "nimble-trx-channel": {
      "orderers": [
        "orderer.example.com"
      ],
      "peers": {
        "peer0.org1.example.com": {
          "endorsingPeer": true,
          "chaincodeQuery": true,
          "ledgerQuery": true,
          "eventSource": true
        },
        "peer1.org1.example.com": {
          "endorsingPeer": true,
          "chaincodeQuery": false,
          "ledgerQuery": true,
          "eventSource": false
        }
      },
      "chaincodes": [
        "mycc:1.0"
      ]
    }
  },
  "organizations": {
    "Org1": {
      "mspid": "Org1MSP",
      "peers": [
        "peer0.org1.example.com",
        "peer1.org1.example.com"
      ],
      "certificateAuthorities": [
        "ca.org1.example.com"
      ],
      "adminPrivateKey": {
        "pem": "-----BEGIN PRIVATE KEY-----\nMIGHAgEAMBMGByqGSM49AgEGCCqGSM49AwEHBG0wawIBAQQgfx8NyMeQ5QQaR1rQ4hEENnS8/FKvL33Cnxlx3hUbLYqhRANCAAT3PXK44ZwKuAMIF4dDyvHDRUVan2mjfhhU2ACoxSgVAWOROCNWaJbIGVTaf/TBEmSgXKSv7EigfkZyr8V5OPYS\n-----END PRIVATE KEY-----"
      },
      "signedCert": {
        "path": "src/main/resources/Admin@org1.example.com-cert.pem"
      }
    },
    "Org2": {
      "mspid": "Org2MSP",
      "peers": [
        "peer0.org2.example.com"
      ]
    }
  },
  "orderers": {
    "orderer.example.com": {
      "url": "grpcs://localhost:7050",
      "grpcOptions": {
        "sslProvider": "openSSL",
        "ssl-target-name-override": "orderer.example.com",
        "grpc-max-send-message-length": 15,
        "negotiationType": "TLS",
        "hostnameOverride": "orderer.example.com"
      },
      "tlsCACerts": {
        "pem": "-----BEGIN CERTIFICATE-----\\n-----END CERTIFICATE-----"
      }
    }
  },
  "peers": {
    "peer0.org1.example.com": {
      "url": "grpcs://localhost:7051",
      "eventUrl": "grpcs://localhost:7053",
      "grpcOptions": {
        "ssl-target-name-override": "peer0.org1.example.com",
        "grpc.http2.keepalive_time": 15,
        "sslProvider": "openSSL",
        "negotiationType": "TLS",
        "hostnameOverride": "peer0.org1.example.com"
      },
      "tlsCACerts": {
        "path": "src/main/resources/org1/server.crt"
      }
    },
    "peer1.org1.example.com": {
      "url": "grpcs://localhost:8051",
      "eventUrl": "grpcs://localhost:8053",
      "grpcOptions": {
        "ssl-target-name-override": "peer1.org1.example.com",
        "sslProvider": "openSSL",
        "negotiationType": "TLS",
        "hostnameOverride": "peer1.org1.example.com"
      },
      "tlsCACerts": {
        "path": "src/main/resources/org2/server.crt"
      }
    }
  },
  "certificateAuthorities": {
    "ca.org1.example.com": {
      "url": "https://localhost:7054",
      "httpOptions": {
        "verify": true
      },
      "tlsCACerts": {
        "path": "src/main/resources/ca.org1.example.com-cert.pem"
      },
      "registrar": [
        {
          "enrollId": "admin",
          "enrollSecret": "adminpw"
        }
      ],
      "caName": "ca.org1.example.com"
    }
  }
}